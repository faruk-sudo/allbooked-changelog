{
  "name": "allbooked-changelog",
  "version": "0.1.0",
  "private": true,
  "description": "AllBooked internal What's New foundation",
  "type": "commonjs",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "db:migrate": "tsx scripts/db/migrate.ts",
    "db:migrate:down": "tsx scripts/db/migrate-down.ts",
    "db:seed:dev": "tsx scripts/db/seed-dev.ts",
    "db:smoke-check": "tsx scripts/db/smoke-check.ts",
    "db:smoke": "tsx scripts/db/smoke-check.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "smoke": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-skedda:faruk-sudo/allbooked-changelog.git"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "markdown-it": "^14.1.0",
    "pg": "^8.18.0",
    "sanitize-html": "^2.17.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^24.5.2",
    "@types/pg": "^8.16.0",
    "@types/sanitize-html": "^2.16.0",
    "@types/supertest": "^6.0.3",
    "supertest": "^7.1.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
